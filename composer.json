{
    "name": "laravel-tools/laravue-generator",
    "description": "A package for generating Vue projects right inside of your Laravel site.",
    "type": "package",
    "license": "MIT",
    "keywords": ["vue", "laravel", "generator", "webpack"],
    "homepage": "https://codetheorist.visualstudio.com/Laravue%20Generator",
    "authors": [
        {
            "name": "Alex Scott",
            "email": "codetheorist@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {"LaravelTools\\LaravueGenerator\\": "src"}
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelTools\\LaravueGenerator\\": "tests"
        }
    },
    "require": {
        "czproject/git-php": "^3.12",
        "laravel/framework": "^5.4",
        "league/flysystem": "^1.0",
        "php": "~5.6|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "LaravelTools\\LaravueGenerator\\LaravueGeneratorServiceProvider"
            ],
            "aliases": {
                "LaravueGenerator": "LaravelTools\\LaravueGenerator\\Facades\\LaravueGenerator"
            }
        }
    },
    "config": {
        "sort-packages": true
    }
}
